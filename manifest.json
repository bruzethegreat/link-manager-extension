{
    "manifest_version": 3,
    "name": "Social Links Manager",
    "version": "1.0",
    "description": "Manage and copy your social media profile links with ease",
    "permissions": ["storage", "clipboardWrite"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
      }
    },
    "icons": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  }